function HideShow(control, divName)
{
  if(control.title == "collpse")
  {
    control.title = "Expand";
	control.src = "../images/up.jpg";
    document.getElementById(divName).style.display = "none";
  }
  else
  {
    control.title = "collpse";
	control.src = "../images/down.jpg";
    document.getElementById(divName).style.display = "block";
  } 
}
