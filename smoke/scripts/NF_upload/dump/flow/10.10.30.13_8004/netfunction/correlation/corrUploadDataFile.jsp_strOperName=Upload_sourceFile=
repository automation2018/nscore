<script language="javascript">






  




window.onbeforeunload = confirmExit

function confirmExit()
{
  if (warnMessage != null) 
  {
    return warnMessage;
  }
}

var warnMessage = null;
jQuery(document).ready(function() {
   jQuery('input:not(:button,:submit),textarea,select').change(function () {

	//window.opener.isDataChanged = true;	
        warnMessage = "You have unsaved changes on this page!";
	//alert("The value of the flag is "+isDataChanged)
    });
    jQuery('input:submit').click(function(e) {
        warnMessage = null;
    });
});




function onLoadFunction()
{

 //alert("test");
}


function validate()
{
  //alert(opener.document.getElementById('destFile') frmMain);
  //opener.getElementById('destFile').value = "Arun Goel";

  if(document.frmMap.FileUpload.value == "")
  {
    alert("Please browse the file");
    return false;
  }
 
  return true;
  
  // if(!document.frmMap.FileUpload.value == "")
  //   submitJSP("browser", document.frmMap.FileUpload.value)
  
}
  
function finish()
{
  var value = "&strMode=addFileParam&rowId=-1";
  
  var frameName = "Right_Frame_Parameter";
  if('' == '')
  {
    if('corrEditFileParam.jsp' == "corrViewURL.jsp")
    {
      frameName = "Hpd_Right_Frame";
      value = "&hostName=" + 'null';
    }
    //alert("delimeter = "+',');
    window.open('corrEditFileParam.jsp'+"?destFile="+ 'LSImport1Prd10ColorwayEach_1.xls' +"&strAction=finish" + "&uploadCheck="+'true' + value  + "&deli="+ ',', frameName);
    window.close();
  }
  else
  {
    window.opener.location.href = 'corrEditFileParam.jsp'+"?destFile="+ 'LSImport1Prd10ColorwayEach_1.xls' +"&strAction=finish" + value + "&deli="+ ','  + "&uploadCheck="+'true';
    window.focus();
    window.close();
  }
}

</script>

