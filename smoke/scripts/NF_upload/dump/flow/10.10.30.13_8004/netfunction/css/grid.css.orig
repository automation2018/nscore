#tabs-2 h1{text-align:center; font-size:22px;}
#tabs-2 p{text-align:justify; font-size:10px;}

#tabs-3 h1{text-align:center; font-size:22px;}
#tabs-3 p{text-align:justify; font-size:10px;}


